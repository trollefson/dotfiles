alias postgres_start="postgres -D /usr/local/var/postgres"
alias mongodb_start="mongod --dbpath /Users/trollefson/Data/mongo/db"
alias redis_start="redis-server /usr/local/etc/redis.conf"
alias ls="ls -la -G"
alias delete_pyc="find . | grep -E '(__pycache__|\.pyc|\.pyo$)' | xargs rm -rf"
alias ssh_add="ssh-add -K ~/.ssh/id_rsa"
alias socks_on="networksetup -setsocksfirewallproxy Wi-Fi localhost 8080"
alias socks_off="networksetup -setsocksfirewallproxystate Wi-Fi off"
alias clean_docker="rm ~/Library/Containers/com.docker.docker/Data/com.docker.driver.amd64-linux/Docker.qcow2"
